{
  "name": "beauty_shop",
  "version": "1.0.0",
  "description": "beauty_shop backend",
  "author": "Maria Belyaeva",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf ./dist/",
    "build": "nest build",
    "build:ci": "nest build",
    "test": "jest",
    "start": "nest start",
    "start:dev": "nest start --watch --exec \"node --max-http-header-size=2000000\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./dist/main",
    "eslint": "eslint \"src/**/*.ts\" --fix",
    "eslint:ci": "eslint \"src/**/*.ts\"",
    "migrate:create": "node-pg-migrate create",
    "migrate": "node-pg-migrate",
    "migrate-up": "node-pg-migrate up --single-transaction",
    "migrate-up:local": "node-pg-migrate up --single-transaction -f \"./config/local.json\""
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "10.3.6",
    "@nestjs/platform-express": "10.3.6",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/sequelize": "10.0.1",
    "@nestjs/swagger": "7.3.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nanoid": "3.3.4",
    "pdfkit-table": "^0.1.99",
    "pg": "8.13.0",
    "pg-hstore": "2.3.4",
    "request-ip": "^3.3.0",
    "rimraf": "3.0.2",
    "sequelize": "6.37.4",
    "sequelize-typescript": "2.1.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@nestjs/cli": "9.4.2",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "4.17.13",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.11.22",
    "@types/request-ip": "^0.0.41",
    "@types/sequelize": "4.28.20",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "sequelize-cli": "6.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.0.4"
  }
