  "scripts": {
    "prebuild": "rimraf ./dist/",
    "build": "nest build",
    "build:ci": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch --exec \"node --max-http-header-size=2000000\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./dist/main",
    "eslint": "eslint \"src/**/*.ts\" --fix",
    "eslint:ci": "eslint \"src/**/*.ts\"",
    "migrate:create": "node-pg-migrate create",
    "migrate": "node-pg-migrate",
    "migrate-up": "node-pg-migrate up --single-transaction",
    "migrate-up:local": "node-pg-migrate up --single-transaction -f \"./config/local.json\""
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "10.3.6",
    "@nestjs/platform-express": "10.3.6",
