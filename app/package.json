    "node-pg-migrate": "^6.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "3.0.2",
    "swagger-ui-express": "4.3.0",
    "joi": "17.11.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.2",
    "@nestjs/schematics": "9.1.0",
